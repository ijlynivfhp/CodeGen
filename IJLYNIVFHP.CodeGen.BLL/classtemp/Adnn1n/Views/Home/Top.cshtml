                <li class="layui-nav-item layui-this"><a href="/Adnn1n/Home/Welcome" target="rightFrame" >后台首页</a></li> 
            </ul>
            <ul class="layui-nav layui-layout-right">
                <!--<li class="layui-nav-item"><a href="/test/config.html" target="_blank">打印配置</a></li>-->
                <li class="layui-nav-item"><a href="/" target="_blank">前台首页</a></li>
                <li class="layui-nav-item"><a href="/Adnn1n/ModPwd/Index" target="rightFrame" >修改密码</a></li>
                <!--<li class="layui-nav-item"><a href="">通知(<strong style="color: red;">12</strong>)</a></li>-->
                <li class="layui-nav-item">
                    <a href="javascript:;"> 
                        @ViewBag.username
                    </a> 
                </li>
                <li class="layui-nav-item"><a href="/adnn1n/login" target="_parent">退出</a></li>
            </ul>
        </div>
 
 

    </div>
    <script src="~/Content/layui2/layui.js"></script>
 
    <script type="text/javascript">
        layui.use('element', function () { 
        });
    </script>

 

